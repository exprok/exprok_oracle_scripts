
On Error Resume Next                                                                                          
strComputer = "amy.ad.vz"                                                                                 
Set colAccounts = GetObject("WinNT://" & strComputer & "")                                                    
Set oWshNet = CreateObject("WScript.Network" )                                                                
sNetBIOSDomain = oWshNet.UserDomain                                                                           
                                                                                                              
Set objUser = colAccounts.Create("group","ora_work_R_120_VVW2_d")                                             
objUser.SetInfo                                                                                               
On Error Resume Next                                                                                          
Set oGroup = GetObject("WinNT://"  &  strComputer & "/ora_work_R_120_VVW2_d,group" )                          
Set oUser = GetObject("WinNT://AD/" &"ora_prog_R_120_VVW2_d" & ",group" )                                     
oGroup.Add(oUser.ADsPath)                                                                                     
