select * 
from w_kts.v_oo_mn v, w_bnh.v_dce_in_shifr sh
where v.nn in ('01111111','02222222','04444444','08888888','03333333','05555555','06666666','07777777','09999999','11111111','22222222','44444444','88888888','33333333','55555555','66666666','77777777','99999999')
      and v.dce=sh.DCE and v.vi=sh.VI
order by nn